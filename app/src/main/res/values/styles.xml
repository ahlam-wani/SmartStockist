<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="TextAppearance.App.Body2" parent="TextAppearance.MaterialComponents.Body1">
        <item name="android:textColor">@color/textColor</item>
        <item name="android:fontFamily">@font/raleway_regular</item>
        <item name="textAppearanceButton">@style/AppTextAppearance.Button</item>
        <item name="textAppearanceHeadline1">@style/AppTextAppearance.Headline1</item>
        <item name="textAppearanceHeadline2">@style/AppTextAppearance.Headline2</item>
        <item name="textAppearanceHeadline3">@style/AppTextAppearance.Headline3</item>
        <item name="textAppearanceHeadline4">@style/AppTextAppearance.Headline4</item>
        <item name="textAppearanceHeadline5">@style/AppTextAppearance.Headline5</item>
        <item name="textAppearanceHeadline6">@style/AppTextAppearance.Headline6</item>
        <item name="textAppearanceSubtitle1">@style/AppTextAppearance.Subtitle1</item>
        <item name="textAppearanceSubtitle2">@style/AppTextAppearance.Subtitle2</item>
        <item name="textAppearanceBody1">@style/AppTextAppearance.Body1</item>
        <item name="textAppearanceBody2">@style/AppTextAppearance.Body2</item>
        <item name="textAppearanceCaption">@style/AppTextAppearance.Caption</item>
        <item name="textAppearanceOverline">@style/AppTextAppearance.Overline</item>
    </style>

    <style name="AppTextAppearance.Headline1" parent="TextAppearance.MaterialComponents.Headline1">
        <item name="android:textSize">40sp</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:lineSpacingExtra">16sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>
    <style name="AppTextAppearance.Headline2" parent="TextAppearance.MaterialComponents.Headline2">
        <item name="android:textSize">32sp</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:lineSpacingExtra">16sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>

    <style name="AppTextAppearance.Headline3" parent="TextAppearance.MaterialComponents.Headline3">
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>

    <style name="AppTextAppearance.Headline4" parent="TextAppearance.MaterialComponents.Headline4">
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:lineSpacingExtra">12sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>

    <style name="AppTextAppearance.Headline5" parent="TextAppearance.MaterialComponents.Headline5">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>

    <style name="AppTextAppearance.Headline6" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textColor">#4f5661</item>
    </style>

    <style name="AppTextAppearance.Body1" parent="TextAppearance.MaterialComponents.Body1" />

    <style name="AppTextAppearance.Body2" parent="TextAppearance.MaterialComponents.Body2" />

    <style name="AppTextAppearance.Subtitle1" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:fontFamily">@font/raleway_regular</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/textColor</item>
    </style>Roboto-Light

    <style name="AppTextAppearance.Subtitle1_roboto" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:fontFamily">@font/raleway_regular</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/textColor</item>
    </style>

    <style name="AppTextAppearance.Subtitle1.Bold" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">#58595b</item>
    </style>

    <style name="AppTextAppearance.Subtitle2.Small" parent="TextAppearance.MaterialComponents.Subtitle2">
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:fontFamily">@font/raleway_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">#58595b</item>
    </style>
    <style name="AppTextAppearance.Subtitle2" parent="TextAppearance.MaterialComponents.Subtitle2">
        <item name="fontFamily">@font/raleway_regular</item>
        <item name="android:fontFamily">@font/raleway_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">#58595b</item>
    </style>
    <style name="AppTextAppearance.Subtitle2.Bold" parent="TextAppearance.MaterialComponents.Subtitle2">
        <item name="fontFamily">@font/raleway_bold</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">#58595b</item>
    </style>
    <style name="AppTextAppearance.title.Bold" parent="TextAppearance.MaterialComponents.Subtitle2">
        <item name="fontFamily">@font/raleway_extrabold</item>
        <item name="android:fontFamily">@font/raleway_extrabold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">#58595b</item>
    </style>
    <style name="AppTextAppearance.Button" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="AppTextAppearance.Caption" parent="TextAppearance.MaterialComponents.Caption" />

    <style name="AppTextAppearance.Overline" parent="TextAppearance.MaterialComponents.Overline" />
</resources>